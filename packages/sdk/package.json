{
  "name": "s4kit",
  "version": "0.1.8",
  "description": "The lightweight, type-safe SDK for SAP S/4HANA",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "s4kit": "./dist/cli.js"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "sap",
    "s4hana",
    "sdk",
    "odata",
    "erp",
    "typescript",
    "api-client"
  ],
  "author": "MichaÅ‚ Majer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michal-majer/s4kit.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/michal-majer/s4kit/issues"
  },
  "homepage": "https://github.com/michal-majer/s4kit#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "ky": "^1.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
