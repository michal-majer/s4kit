    "ky": "^1.14.3",
    "postgres": "^3.4.8",
    "resend": "^6.9.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/ioredis": "^5.0.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9"
